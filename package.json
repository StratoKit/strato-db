{
  "name": "strato-db",
  "version": "3.5.2",
  "description": "NoSQL-hybrid with Event Sourcing based on sqlite",
  "license": "MIT",
  "repository": "https://github.com/StratoKit/strato-db",
  "author": {
    "name": "Wout Mertens",
    "email": "Wout.Mertens@gmail.com"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist-types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist-types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "dist-types",
    "Readme.md"
  ],
  "engines": {
    "node": ">=16.4"
  },
  "scripts": {
    "start": "nps",
    "test": "nps test",
    "prepublishOnly": "nps test.full build"
  },
  "keywords": [
    "sqlite",
    "nosql",
    "event sourcing",
    "esdb",
    "jsonmodel",
    "json",
    "database",
    "db"
  ],
  "dependencies": {
    "async-sema": "^3",
    "dataloader": "^2",
    "debug": ">=2 <5",
    "jsurl2": "^2",
    "lodash": "^4",
    "prop-types": "^15",
    "sqlite3": "^5",
    "uuid": ">=8 <10"
  },
  "devDependencies": {
    "@homer0/prettier-plugin-jsdoc": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitest/coverage-c8": "^0.25.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-plugin-jsdoc": "^40.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unicorn": "^45.0.2",
    "eslint-plugin-vitest-globals": "^1.2.0",
    "fs-extra": "^11.1.0",
    "nps-utils": "^1.0.2",
    "prettier": "^2.8.0",
    "tmp-promise": "3.0.3",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vitest": "^0.25.3"
  },
  "jest": {
    "globals": {
      "__PRODUCTION__": false
    },
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "_test-helpers.js"
    ]
  }
}
